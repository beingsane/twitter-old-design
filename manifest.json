{
  "manifest_version": 2,
  "name": "Twitter - Old design",
  "version": "1.0.1",
  "minimum_chrome_version": "15",
  "icons": {
    "16": "assets/icon_16.png",
    "48": "assets/icon_48.png",
    "128": "assets/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://twitter.com/*",
        "*://*.twitter.com/*"
      ],
      "js": [
        "src/content_script.js"
      ],
      "run_at": "document_idle"
    }
  ]
}
